    <eStructuralFeatures xsi:type="ecore:EReference" name="outputport" lowerBound="1"
        upperBound="-1" eType="#//OutputPort" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="State">
